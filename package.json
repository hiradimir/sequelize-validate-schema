{
  "name": "sequelize-validate-schema",
  "version": "0.0.9",
  "description": "check defined sequelize models and db-schema",
  "private": false,
  "engines": {
    "node": ">=7.0.0",
    "npm": ">=4.0.0"
  },
  "main": "dist/js/main/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/hiradimir/sequelize-validate-schema.git"
  },
  "author": "hiradimir",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hiradimir/sequelize-validate-schema/issues"
  },
  "homepage": "https://github.com/hiradimir/sequelize-validate-schema",
  "scripts": {
    "clean": "rimraf dist/",
    "prebuild": "npm run clean",
    "tsc": "tsc",
    "build": "npm run tsc",
    "prepare": "npm run build",
    "test": "export NODE_ENV=test && nyc mocha --opts src/ts/spec/mocha.opts"
  },
  "directories": {
    "test": "src/spec"
  },
  "nyc": {
    "include": [
      "src/ts/**/*.ts"
    ],
    "exclude": [
      "src/ts/spec/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "lcov",
      "text",
      "html",
      "cobertura"
    ],
    "report-dir": "dist/report",
    "sourceMap": true,
    "instrument": true
  },
  "dependencies": {
    "bluebird": "^3.5.5",
    "lodash": "^4.17.15",
    "mysql2": "^1.6.5",
    "sequelize": "^5.21.3"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.20",
    "@types/lodash": "^4.14.104",
    "@types/mocha": "^2.2.48",
    "@types/node": "^9.4.6",
    "@types/sequelize": "^4.28.8",
    "@types/validator": "^12.0.1",
    "espower-typescript": "^8.1.3",
    "istanbul": "^0.4.5",
    "mocha": "^5.0.1",
    "mocha-multi": "^1.0.1",
    "nyc": "^11.4.1",
    "pg": "^7",
    "pg-hstore": "^2.3.2",
    "power-assert": "^1.4.4",
    "remap-istanbul": "^0.10.1",
    "rimraf": "^2.6.2",
    "sequelize-typescript": "^1.1.0",
    "ts-node": "^5.0.0",
    "typescript": "^3.7.2"
  },
  "optionalDependencies": {
    "fsevents": "^1.1.3"
  }
}
